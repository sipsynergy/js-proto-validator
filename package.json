{
  "name": "@sipsynergy/proto-valid",
  "version": "0.6.0",
  "main": "dist/proto-valid.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sipsynergy/js-proto-validator.git"
  },
  "bin": "parser/parser.js",
  "license": "MIT",
  "private": true,
  "scripts": {

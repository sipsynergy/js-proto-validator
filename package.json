    "minimist": "^1.2.0",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "pegjs": "^0.10.0",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^3.2.2"
  },
  "optionalDependencies": {
    "whatwg-fetch": "^2.0.3"
  }
}
